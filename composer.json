{
  "name": "fagundes/ionic-cloud-api-php",
  "type": "library",
  "description": "Client library for Ionic Cloud APIs",
  "keywords": [
    "ionic",
    "cloud",
    "api",
    "notifications",
    "push",
    "messages"
  ],
  "homepage": "http://github.com/fagundes/ionic-cloud-api-php",
  "license": "Apache-2.0",
  "require": {
    "php": ">=5.6",
    "guzzlehttp/guzzle": "~5.2|~6.0",
    "guzzlehttp/psr7": "^1.2",
    "google/auth": "^0.11"
  },
  "require-dev": {
    "phpunit/phpunit": "~4",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "suggest": {
  },
  "autoload": {
    "psr-4": {
      "Ionic\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "serve": "php -S 0.0.0.0:8080 -t examples/",
    "test": "phpunit"
  }
}
